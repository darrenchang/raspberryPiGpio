import RPi.GPIO as GPIO
import time

GPIO.setmode(GPIO.BOARD)

GPIO.setup(8, GPIO.OUT)

try:
    while True:
        GPIO.output(8, 1)
        time.sleep(0.5)
        GPIO.output(8, 0)
        time.sleep(0.5)

except (KeyboardInterrupt):
    print "A keyboard interrupt has been notitced"

except:
    print "An error or exception has occured!"

finally:
    GPIO.cleanup()
